        },
        {
          "type": "select",
          "value": "garlic",
          "label": "Garlic"
        },
        {
          "type": "select",
          "value": "stuffed",
          "label": "Stuffed"
        },
        {
          "type": "select",
          "value": "deepdish",
          "label": "Deep Dish"
        }
      ],
      "comment": {
        "type": "textarea",
        "title": "Any additional preferences",
        "form": "orderForm",
        "rows": "5",
        "cols": "80",
        "name": "comment",
        "label": "Comment"
      }
    },
