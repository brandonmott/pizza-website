{
  "pizza": {
    "customer": {
      "form": {
        "id": "newUserForm",
        "action": "#",
        "method": "POST"
      },
      "div": [
        {
          "head": "customer",
          "class": ".col-xs-12 col-sm-12 .col-md-3 col-lg-4"
        },
        {
          "head": "Customer",
          "class": "panel panel-default panel-success"
        },
        {
          "head": "New customer sign up",
          "class": "panel-heading"
        }
      ],
      "input": [
        {
          "type": "text",
          "name": "name",
          "title": "Please enter full name",
          "label": "Name"
        },
        {
          "type": "text",
          "name": "phone",
          "title": "Please enter phone number",
          "label": "Phone"
        },
        {
          "type": "text",
          "name": "email",
          "title": "Please enter your email",
          "label": "Email"
        },
        {
          "type": "text",
          "name": "address",
          "title": "Please enter your home address",
          "label": "Address"
        },
        {
          "type": "text",
          "name": "city",
          "title": "Please enter the city you live in",
          "label": "City"
        },
        {
          "type": "text",
          "name": "zip",
          "title": "Please enter your zip code",
          "label": "Zip"
        },
        {
          "type": "text",
          "name": "username",
          "title": "Please enter your username",
          "label": "Username <br>(Use this to log in to your account)"
        },
        {
          "type": "text",
          "name": "password1",
          "title": "Please enter your password",
          "label": "Password"
        },
        {
          "type": "text",
          "name": "password2",
          "title": "Please re-enter your password",
          "label": "Confirm Password"
        }
      ],
      "buttons": [
        {
          "type": "submit",
          "name": "signUp",
          "id": "signUp",
          "label": "Sign up",
          "class": "btn btn-success",
          "spanStyle": "color:#FFFFFF;",
          "spanClass": "glyphicons glyphicons-check"
        },
        {
          "type": "",
          "name": "signIn",
          "id": "signIn",
          "label": "Sign in",
          "class": "btn btn-primary",
          "spanStyle": "color:#FFFFFF;",
          "spanClass": "glyphicons glyphicons-new-window"
        }
      ]
    },
    "size": {
      "form": {
        "id": "orderForm",
        "action": "#",
        "method": "POST"
      },
      "div": [
        {
          "head": "size",
          "class": "col-xs-12 col-sm-4 col-md-2 col-lg-2"
        },
        {
          "head": "Size",
          "class": "panel panel-default panel-warning"
        },
        {
          "head": "What Size do you want your pizza?",
          "class": "panel-heading"
        }
      ],
      "input": [
        {
          "type": "radio",
          "name": "size",
          "value": "small",
          "label": "Small"
        },
        {
          "type": "radio",
          "name": "size",
          "value": "medium",
          "label": "Medium"
        },
        {
          "type": "radio",
          "name": "size",
          "value": "large",
          "label": "Large"
        },
        {
          "type": "radio",
          "name": "size",
          "value": "family",
          "label": "Family"
        }
      ]
    },
    "crust": {
      "div": [
        {
          "head": "crust",
          "class": "col-xs-12 col-sm-7 col-md-6 col-lg-6"
        },
        {
          "head": "Crust",
          "class": "panel panel-default panel-danger"
        },
        {
          "head": "How do you like your crust?",
          "class": "panel-heading"
        }
      ],
      "input": [
        {
          "type": "select",
          "value": "regular",
          "label": "Regular"
        },
        {
          "type": "select",
          "value": "thin",
          "label": "Thin"
        },
        {
          "type": "select",
          "value": "pretzel",
